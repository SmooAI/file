{
    "name": "@smooai/file",
    "version": "1.1.5",
    "description": "A powerful file handling library for Node.js that provides a unified interface for working with files from local filesystem, S3, URLs, and more. Built with streaming in mind, it handles file bytes lazily where possible to minimize memory usage and improve performance.",
    "homepage": "https://github.com/SmooAI/file#readme",
    "bugs": {
        "url": "https://github.com/SmooAI/file/issues"
    },
    "license": "MIT",
    "author": {
        "name": "SmooAI",
        "email": "brent@smooai.com",
        "url": "https://smooai.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SmooAI/file.git"
    },
    "files": [
        "dist/**"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.mjs",
            "require": "./dist/*.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "aibranch": "pnpm generate-git-branch",
        "build": "pnpm create-entry-points -i \"src/**/*.ts\" && pnpm tsup && pnpm run python:build && pnpm run rust:build && pnpm run go:build",
        "check-all": "pnpm run typecheck && pnpm run lint && pnpm run format:check && pnpm run test && pnpm run python:typecheck && pnpm run python:lint && pnpm run python:format:check && pnpm run rust:fmt:check && pnpm run rust:lint && pnpm run rust:test && pnpm run go:fmt:check && pnpm run go:lint && pnpm run go:test && pnpm run build",
        "ci:publish": "pnpm build && pnpm changeset publish && pnpm run version:sync",
        "format": "oxfmt --write . && pnpm run python:format && pnpm run rust:fmt && pnpm run go:fmt",
        "lint": "oxlint . && pnpm run python:lint && pnpm run rust:lint && pnpm run go:lint",
        "lint:fix": "oxlint --fix .",
        "pre-commit-check": "(zsh .husky/pre-commit)",
        "prepare": "husky",
        "test": "vitest run && pnpm python:test && pnpm rust:test && pnpm go:test",
        "test:integration": "vitest run File.integration.spec",
        "typecheck": "tsc --noEmit --skipLibCheck && pnpm run python:typecheck && pnpm run rust:check",
        "watch": "tsup --watch",
        "format:check": "oxfmt --check .",
        "python:build": "(cd python && uv run poe build)",
        "python:format": "(cd python && uv run poe format)",
        "python:format:check": "(cd python && uv run poe format:check)",
        "python:install-dev": "(cd python && uv run poe install-dev)",
        "python:lint": "(cd python && uv run poe lint)",
        "python:lint:fix": "(cd python && uv run poe lint:fix)",
        "python:test": "(cd python && uv run poe test)",
        "python:typecheck": "(cd python && uv run poe typecheck)",
        "rust:build": "(cd rust/file && cargo build --release)",
        "rust:check": "(cd rust/file && cargo check --all-targets)",
        "rust:fmt": "(cd rust/file && cargo fmt)",
        "rust:fmt:check": "(cd rust/file && cargo fmt --all -- --check)",
        "rust:lint": "(cd rust/file && cargo clippy --all-targets -- -D warnings)",
        "rust:test": "(cd rust/file && cargo test)",
        "go:build": "(cd go/file && go build ./...)",
        "go:fmt": "(cd go/file && gofmt -w .)",
        "go:fmt:check": "(cd go/file && test -z \"$(gofmt -l .)\")",
        "go:lint": "(cd go/file && go vet ./...)",
        "go:test": "(cd go/file && go test -v ./...)",
        "version:sync": "node scripts/sync-versions.mjs"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.777.0",
        "@aws-sdk/s3-request-presigner": "^3.777.0",
        "@file-type/xml": "^0.4.1",
        "@smooai/fetch": "^2.1.0",
        "@smooai/logger": "^3.1.2",
        "@smooai/utils": "^1.3.1",
        "content-disposition": "^0.5.4",
        "file-type": "^20.4.1",
        "formdata-node": "^6.0.3",
        "mime-types": "^2.1.35",
        "tiny-invariant": "^1.3.3",
        "typescript": "^5.9.2"
    },
    "devDependencies": {
        "@changesets/cli": "^2.28.1",
        "@smooai/config-typescript": "^1.0.16",
        "@types/content-disposition": "^0.5.8",
        "@types/mime-types": "^2.1.4",
        "@types/node": "^22.13.10",
        "husky": "^9.1.7",
        "msw": "^2.7.3",
        "oxfmt": "^0.28.0",
        "oxlint": "^0.16.0",
        "tsup": "^8.4.0",
        "vite": "^6.2.3",
        "vite-node": "^3.0.9",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.9",
        "vitest-tsconfig-paths": "^3.4.1"
    },
    "packageManager": "pnpm@10.6.1+sha512.40ee09af407fa9fbb5fbfb8e1cb40fbb74c0af0c3e10e9224d7b53c7658528615b2c92450e74cfad91e3a2dcafe3ce4050d80bda71d757756d2ce2b66213e9a3"
}
